<Window x:Class="PhoneBook.Image"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhoneBook"
        mc:Ignorable="d"
        Title="Image" Height="387" Width="599" MinWidth="599" MinHeight="387">
    <Grid>
        <Rectangle Height="56" Margin="1,1,1,1" Stroke="Black" VerticalAlignment="Top" Fill="#FFEEF1DC"/>
        <Button x:Name="btnLoad" Content="Load" HorizontalAlignment="Left" Margin="82,16,0,0" VerticalAlignment="Top" Height="27" Width="64" Click="btnLoad_Click"/>
        <Image x:Name="image" Margin="1,59,2,1" Source="/6.jpg" Stretch="UniformToFill"/>
        <Button x:Name="btnFill" Content="Fill" HorizontalAlignment="Left" Margin="432,16,0,0" VerticalAlignment="Top" Width="64" Height="27" Click="btnFill_Click"/>
        <Button x:Name="btnFill_Copy" Content="Zoom" HorizontalAlignment="Left" Margin="510,16,0,0" VerticalAlignment="Top" Width="64" Height="27" Click="btnFill_Copy_Click"/>
        <Button x:Name="btnBrowse" Content="Browse" HorizontalAlignment="Left" Margin="8,16,0,0" VerticalAlignment="Top" Height="27" Width="64" Click="btnBrowse_Click" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="161,17,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="258" Height="24" TextChanged="textBox_TextChanged" />

    </Grid>
</Window>
