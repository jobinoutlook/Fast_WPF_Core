<Window x:Class="PhoneBook.AddData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhoneBook"
        mc:Ignorable="d"
        Title="Add Data" Height="488" Width="389" WindowStartupLocation="CenterScreen" Icon="/users_icon.png" FontWeight="Bold">
    <Grid Background="#FFE7E7D7">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="111,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" Height="22" FontSize="14" FontWeight="Bold" TabIndex="0"/>
        <Label x:Name="label" Content="ID" HorizontalAlignment="Left" Margin="27,22,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <Label x:Name="label_Copy" Content="First Name" HorizontalAlignment="Left" Margin="27,62,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Margin="111,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" Height="22" FontSize="14" FontWeight="Bold" TabIndex="1"/>
        <Label x:Name="label_Copy1" Content="Last Name" HorizontalAlignment="Left" Margin="27,109,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <TextBox x:Name="textBox_Copy1" HorizontalAlignment="Left" Margin="110,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="211" Height="22" FontSize="14" FontWeight="Bold" TabIndex="2"/>
        <Label x:Name="label_Copy2" Content="Cell Number" HorizontalAlignment="Left" Margin="27,189,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <TextBox x:Name="textBox_Copy2" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" Height="22" Margin="112,192,0,0" FontSize="14" FontWeight="Bold" TabIndex="4"/>
        <Label x:Name="label_Copy3" Content="Land Line" HorizontalAlignment="Left" Margin="27,236,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <TextBox x:Name="textBox_Copy3" HorizontalAlignment="Left" Margin="109,236,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="212" Height="22" TextChanged="textBox_Copy3_TextChanged" FontSize="14" FontWeight="Bold" TabIndex="5"/>
        <Label x:Name="label_Copy4" Content="Email" HorizontalAlignment="Left" Margin="27,272,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <TextBox x:Name="textBox_Copy4" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Width="211" Height="22" Margin="110,273,0,0" FontSize="14" FontWeight="Bold" TabIndex="6"/>
        <Label x:Name="label_Copy5" Content="Comment" HorizontalAlignment="Left" Margin="27,340,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <TextBox x:Name="textBox_Copy5" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Width="211" Height="22" Margin="111,343,0,0" FontSize="14" FontWeight="Bold" TabIndex="7"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="110,151,0,0" VerticalAlignment="Top" Width="208" SelectedIndex="0" Cursor="Hand" FontWeight="Bold" FontSize="14" TabIndex="3">
            <ComboBoxItem Cursor="Hand" Content="Male"/>
            <ComboBoxItem Cursor="Hand" Content="Female"/>
        </ComboBox>
        <Label x:Name="label_Copy6" Content="Gender" HorizontalAlignment="Left" Margin="27,149,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <Button x:Name="button" Content="Save Changes F5" HorizontalAlignment="Left" Margin="112,384,0,0" VerticalAlignment="Top" Width="208" Click="button_Click" Height="28" Cursor="Hand" TabIndex="8"/>
        <Label x:Name="label_Copy7" Content="Address
               " HorizontalAlignment="Left" Margin="27,306,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.166,-1.463"/>
        <TextBox x:Name="textBox_Copy6" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Width="211" Height="22" Margin="109,309,0,0" FontSize="14" FontWeight="Bold" TabIndex="7"/>
    </Grid>
</Window>
